{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "../dist/repl",
    "rootDir": "./",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "lib": ["es2015", "es2016", "dom", "es2017", "es6", "es5"]
  },
  "exclude": ["node_modules"],
  "atom": {
    "rewriteTsconfig": false
  }
}
